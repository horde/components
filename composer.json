{
    "name": "horde/components",
    "description": "Developer tool for managing Horde components",
    "type": "horde-application",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Gunnar Wrobel",
            "email": "p@rdus.de",
            "role": "lead"
        },
        {
            "name": "Ralf Lang",
            "email": "lang@b1-systems.de",
            "role": "contributor"
        }
    ],
    "time": "2024-01-12",
    "repositories": [],
    "require": {
        "horde/horde-installer-plugin": "^2",
        "php": "^8",
        "horde/autoloader": "*",
        "horde/argv": "*",
        "horde/cli": "^3",
        "horde/cli_modular": "^3",
        "horde/http": "*",
        "horde/pear": "*",
        "horde/role": "*",
        "horde/text_diff": "*",
        "horde/util": "*",
        "horde/injector": "*",
        "horde/release": "^4",
        "horde/yaml": "*",
        "horde/eventdispatcher": "*",
        "horde/githubapiclient": "*"
    },
    "require-dev": {
        "horde/test": "^3"
    },
    "suggest": {
        "horde/test": "^3",
        "squizlabs/php_codesniffer": "^3.5",
        "pear/archive_tar": "^1.4",
        "phploc/phploc": "^7",
        "phpmd/phpmd": "^2.9",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-0": {
            "Components": "lib/"
        },
        "psr-4": {
            "Horde\\Components\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Components\\Test\\": "test/"
        }
    },
    "bin": [
        "bin/horde-components",
        "bin/horde-components-prototype-transpile"
    ],
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}